{
    "name": "evolution7/craft-ckeditor5-plugins",
    "version": "0.1.1",
    "description": "CKEditor 5 plugins collection for Craft CMS",
    "type": "craft-plugin",
    "require": {
        "craftcms/cms": "^5.0.0",
        "craftcms/ckeditor": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "evolution7\\ckeditor5plugins\\": "src/"
        }
    },
    "extra": {
        "name": "CKEditor 5 Plugins",
        "handle": "craft-ckeditor5-plugins",
        "class": "evolution7\\ckeditor5plugins\\Plugin"
    },
    "scripts": {
        "build": "node scripts/copy-builds.js"
    }
}